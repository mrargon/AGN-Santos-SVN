"VertexLitGeneric"
{
	"$baseTexture" "noble/trafficbarrel/barrel.vtf"
	$bumpmap "noble/trafficbarrel/skin.vtf"
	$selfillum "1"
	$selfillumtint "[4 4 4]"

	"$phong"	"1"
	"$phongexponent"	"25"
	"$phongboost"	"1"
	"$phongfresnelranges"	"[0.1 0.1 5]"
	"$halflambert" "1"
	"$rimlight" "1"

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" "2.5"
		}
	}
}