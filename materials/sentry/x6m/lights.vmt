"VertexlitGeneric"
{
	"$basetexture" "sentry/x6m/lights"
	"$bumpmap" "sentry/x6m/lights_nm"
	"$blendtintbybasealpha" 1
	"$nocull" 1
    $envmap "env_cubemap"
    $envmaptint "[.5 .5 .5]"
    $envmapcontrast "1"
    $envmapsaturation ".5"
    $envmapfresnel "1"

    $phong "1"
    $phongexponent "15"
    $phongboost "1"
    $phongfresnelranges "[.05 .3 1]"

	"$carpenisenabled" "1"
	Proxies // proxies are listed inside this block
	{
		Equals // a proxy which produces a penis
		{
			srcVar1 $carpenisenabled
			resultVar	$color // The shader parameter to be manipulated

		}
	}
}
